              </li>
              <li class="nav-item">
                  <a class="nav-link" <%= link_to "Create Account", new_user_path %></a>
              </li>
              <% else %>
            <li class="nav-item">
                <a class="nav-link" <%= link_to "All Listings", listings_path%></a>
            </li>
            <li class="nav-item">
              <a class="nav-link" <%= link_to "View Profile", user_path(session[:user_id])%></a>
            </li>
            <% end %>
          </ul>
